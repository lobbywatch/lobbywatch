    <DataSource name="q_unvollstaendige_parlamentarier" type="query">
      <PrimaryKeyFields>
        <Field name="id"/>
      </PrimaryKeyFields>
      <SelectStatement>
select * from parlamentarier where
geschlecht is null OR
geburtstag is null OR
email is null OR trim(email)='' OR
im_rat_seit is null OR
kleinbild is null OR
parlament_biografie_id is null OR
beruf is null      </SelectStatement>
    </DataSource>
    <DataSource name="q_unvollstaendige_zutrittsberechtigte" type="query">
      <PrimaryKeyFields>
        <Field name="id"/>
      </PrimaryKeyFields>
      <SelectStatement>
select zutrittsberechtigung.*, parlamentarier.kommissionen from
v_zutrittsberechtigung_simple_compat zutrittsberechtigung
LEFT JOIN v_parlamentarier parlamentarier
ON zutrittsberechtigung.parlamentarier_id = parlamentarier.id
where
zutrittsberechtigung.geschlecht is null OR
zutrittsberechtigung.email is null OR trim(zutrittsberechtigung.email)='' OR
zutrittsberechtigung.beruf is null OR trim(zutrittsberechtigung.beruf)='' OR
zutrittsberechtigung.funktion is null OR trim(zutrittsberechtigung.funktion)=''      </SelectStatement>
    </DataSource>
    <DataSource name="q_unvollstaendige_organisationen" type="query">
      <PrimaryKeyFields>
        <Field name="id"/>
      </PrimaryKeyFields>
      <SelectStatement>
select * from organisation where
rechtsform is null OR rechtsform = '' OR
interessengruppe_id = null OR
branche_id = null      </SelectStatement>
    </DataSource>
